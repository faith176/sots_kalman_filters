{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FilteredEvent",
  "type": "object",
  "required": ["streamId", "timestamp", "filteredMeasurement", "quality", "streamInfo"],
  "properties": {
    "streamId": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    },
    "filteredMeasurement": {
      "type": "number"
    },
    "quality": {
      "type": "object",
      "properties": {
        "imputed": { "type": "boolean" },
        "confidence": { 
          "type": "number",
          "minimum": 0.0,
          "maximum": 1.0
        }
      },
      "required": ["imputed", "confidence"]
    },
    "streamInfo": {
      "type": "object",
      "properties": {
        "streamType": { "type": "string" },
        "unit": { "type": "string" },
        "filterSource": { "type": "string" },
        "location": { "type": "string" }
      },
      "required": ["streamType", "unit", "filterSource"]
    }
  },
  "additionalProperties": false
}
